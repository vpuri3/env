#!/bin/bash

vmname=$1
#pname=$2

zn=us-east4-a

pid=proj1-301020

gcloud compute instances create $vmname \
 	--project $pid \
  	--zone us-east4-a \
 	--image-family ubuntu-2004-lts \
 	--image-project ubuntu-os-cloud \
    --metadata-from-file startup-script=$HOME/env/bootstrap.sh
    --metadata enable-oslogin=true
